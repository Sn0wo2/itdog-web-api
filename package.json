{
  "name": "itdog-web-api",
  "description": "A TypeScript library for ping functionality using itdog.cn Web API",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "example": "tsx example/test.ts",
    "prepublishOnly": "pnpm run lint && pnpm run test && pnpm run build"
  },
  "keywords": [
    "ping",
    "itdog",
    "network",
    "typescript",
    "websocket"
  ],
  "author": "Sn0wo2",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.1.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@commitlint/types": "^19.8.1",
    "@eslint/js": "^9.35.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "private": false
}